<script>console.log('%c Welcome to Juejin! ', 'color:#1e80ff;font-size:20px;background:#fff;padding:8px;');
  function fixHydrogen() {
    if (!/^\/s?post\//i.test(location.pathname)) {
      return;
    }
    if (!window.__NUXT__.state.view.column.entry.article_info.mark_content.startsWith('---\ntheme: hydrogen\n')) {
      return;
    }
    const styleEle = document.createElement('style');
    styleEle.innerHTML = '.markdown-body a{display:inline-block;}';
    document.head.appendChild(styleEle);
  }
  window.addEventListener('load', fixHydrogen);</script>

